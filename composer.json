{
    "name": "larangular/installable",
    "description": "Installable",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/console": "~5.8.0|^6.0|^7.0|^8.0",
        "larangular/support": "^0.1.0|^0.2.0",
        "larangular/config-writer": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\Installable\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\Installable\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\Installable\\InstallableServiceProvider"
            ],
            "aliases": {
                "InstallableMigration": "Larangular\\Installable\\Facades\\InstallableMigration"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.8.0|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.4|^8.0"
    }
}
