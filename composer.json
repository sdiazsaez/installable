{
    "name": "larangular/installable",
    "description": "Installable",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.*",
        "illuminate/console": "^5.7",
        "larangular/support": "^0.1.5",
        "grey-dev-0/laravel-config": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\Installable\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\Installable\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\Installable\\InstallableServiceProvider"
            ],
            "aliases": {
                "InstallableMigration": "Larangular\\Installable\\Facades\\InstallableMigration"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.5"
    }
}
